{
	"name": "express-blog",
	"version": "0.1.0",
	"description": "The webpage dominikriedig.de including the cms and cloudcenter",
	"private": "true",
	"main": "dist/wrapper.js",
	"engines": {
		"node": ">=16.15.0"
	},
	"contributors": [
		{
			"name": "Dominik Riedig",
			"email": "my-blog@dominikriedig.de",
			"url": "https://dominikriedig.de"
		}
	],
	"dependencies": {
		"bcrypt": "5.1.0",
		"body-parser": "1.20.2",
		"cheerio": "1.0.0-rc.12",
		"cookie-parser": "1.4.6",
		"dotenv": "16.0.3",
		"ejs": "3.1.8",
		"express": "4.18.2",
		"express-useragent": "1.0.15",
		"ffprobe": "^1.1.2",
		"ffprobe-static": "^3.1.0",
		"helmet": "6.0.1",
		"heroicons": "2.0.16",
		"highlight.js": "11.7.0",
		"moment": "2.29.4",
		"mysql2": "3.2.0",
		"npm-run-all": "4.1.5",
		"pureimage": "0.3.17",
		"reflect-metadata": "0.1.13",
		"sequelize": "6.29.1",
		"sequelize-typescript": "2.1.5",
		"showdown": "2.1.0",
		"socket.io": "^4.5.4",
		"uuid": "9.0.0",
		"winston": "3.8.2"
	},
	"devDependencies": {
		"@tailwindcss/forms": "0.5.3",
		"@tailwindcss/typography": "0.5.9",
		"@types/bcrypt": "5.0.0",
		"@types/cookie-parser": "1.4.3",
		"@types/express": "4.17.17",
		"@types/express-useragent": "1.0.2",
		"@types/ffprobe": "1.1.4",
		"@types/ffprobe-static": "2.0.1",
		"@types/jest": "29.4.0",
		"@types/node": "18.14.6",
		"@types/opentype.js": "1.3.4",
		"@types/shelljs": "0.8.11",
		"@types/showdown": "2.0.0",
		"@types/uuid": "9.0.1",
		"@typescript-eslint/eslint-plugin": "5.54.1",
		"@typescript-eslint/parser": "5.54.1",
		"eslint": "8.35.0",
		"eslint-config-google": "0.14.0",
		"eslint-plugin-node": "11.1.0",
		"jest": "29.5.0",
		"nodemon": "2.0.21",
		"rimraf": "4.3.1",
		"shelljs": "0.8.5",
		"socket.io-client": "^4.5.4",
		"sqlite3": "5.1.4",
		"tailwind-highlightjs": "2.0.1",
		"tailwindcss": "3.2.7",
		"ts-jest": "29.0.5",
		"ts-loader": "9.4.2",
		"ts-node": "10.9.1",
		"typescript": "4.9.5",
		"webpack": "5.76.0",
		"webpack-cli": "5.0.1",
		"workbox-cacheable-response": "6.5.4",
		"workbox-expiration": "6.5.4",
		"workbox-routing": "6.5.4",
		"workbox-strategies": "6.5.4"
	},
	"files": [
		"all"
	],
	"scripts": {
		"dev": "npm-run-all --parallel dev:serve dev:js-webpack dev:js-move dev:tailwind-blog dev:tailwind-cms dev:tailwind-cloudcenter dev:assets",
		"debug": "npm-run-all --parallel debug:serve dev:js-webpack dev:js-move dev:tailwind-blog dev:tailwind-cms dev:tailwind-cloudcenter dev:assets",
		"test": "jest",
		"coverage": "jest --coverage",
		"build": "npm-run-all clean tsc tailwind-blog tailwind-cms tailwind-cloudcenter copy-assets js-wrapper",
		"start": "node .",
		"clean": "rimraf dist/*",
		"copy-assets": "ts-node tools/copyAssets",
		"lint": "eslint src/**/*{.js,.jsx,.ts,.tsx}",
		"lint:fix": "eslint src/**/*{.js,.jsx,.ts,.tsx} --fix",
		"tsc": "tsc",
		"tailwind-blog": "TW_COMPILE_SUBAPP_NAME=blog tailwindcss -i ./src/blog/blog.css -o ./dist/blog/public/blog.css --minify",
		"tailwind-cms": "TW_COMPILE_SUBAPP_NAME=cms tailwindcss -i ./src/cms/cms.css -o ./dist/cms/public/cms.css --minify",
		"tailwind-cloudcenter": "TW_COMPILE_SUBAPP_NAME=cloudcenter tailwindcss -i ./src/cloudcenter/cloudcenter.css -o ./dist/cloudcenter/public/cloudcenter.css --minify",
		"js-wrapper": "npm-run-all js-webpack js-move",
		"js-webpack": "webpack-cli --devtool source-map",
		"js-move": "ts-node tools/jsBrowserAfterCompile",
		"debug:serve": "nodemon --inspect --watch src --ignore src/browser -e ts ./src/wrapper.ts",
		"dev:serve": "nodemon --watch src --ignore src/browser -e ts ./src/wrapper.ts",
		"dev:assets": "nodemon --watch 'src/blog/public/**/*' --watch 'src/cms/public/**/*' --watch 'src/cloudcenter/public/**/*' --exec ts-node tools/copyAssets",
		"dev:js-webpack": "webpack-cli --devtool inline-source-map --watch --progress",
		"dev:js-move": "nodemon --watch 'dist/public/**/*' --exec ts-node tools/jsBrowserAfterCompile",
		"dev:tailwind-blog": "TW_COMPILE_SUBAPP_NAME=blog tailwindcss -i ./src/blog/blog.css -o ./dist/blog/public/blog.css --watch",
		"dev:tailwind-cms": "TW_COMPILE_SUBAPP_NAME=cms tailwindcss -i ./src/cms/cms.css -o ./dist/cms/public/cms.css --watch",
		"dev:tailwind-cloudcenter": "TW_COMPILE_SUBAPP_NAME=cloudcenter tailwindcss -i ./src/cloudcenter/cloudcenter.css -o ./dist/cloudcenter/public/cloudcenter.css --watch"
	},
	"license": "GPLv3"
}
