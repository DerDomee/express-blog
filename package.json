{
	"name": "express-blog",
	"version": "0.1.0",
	"description": "The webpage dominikriedig.de including the cms and cloudcenter",
	"private": "true",
	"main": "dist/wrapper.js",
	"engines": {
		"node": ">=21.0.0"
	},
	"contributors": [
		{
			"name": "Dominik Riedig",
			"email": "my-blog@dominikriedig.de",
			"url": "https://dominikriedig.de"
		}
	],
	"dependencies": {
		"bcrypt": "5.1.1",
		"body-parser": "1.20.2",
		"cheerio": "1.0.0-rc.12",
		"cookie-parser": "1.4.6",
		"dotenv": "16.4.2",
		"ejs": "3.1.9",
		"express": "4.18.2",
		"express-useragent": "1.0.15",
		"ffprobe": "1.1.2",
		"ffprobe-static": "3.1.0",
		"helmet": "7.1.0",
		"heroicons": "2.0.18",
		"highlight.js": "11.9.0",
		"moment": "2.30.1",
		"mysql2": "3.8.0",
		"nodemailer": "6.9.9",
		"npm-run-all": "4.1.5",
		"reflect-metadata": "0.1.14",
		"sequelize": "6.35.2",
		"sequelize-typescript": "2.1.6",
		"showdown": "2.1.0",
		"socket.io": "4.7.4",
		"uuid": "9.0.1",
		"winston": "3.11.0"
	},
	"devDependencies": {
		"@jest/types": "29.6.3",
		"@tailwindcss/container-queries": "0.1.1",
		"@tailwindcss/forms": "0.5.7",
		"@tailwindcss/typography": "0.5.10",
		"@types/bcrypt": "5.0.2",
		"@types/cookie-parser": "1.4.6",
		"@types/express": "4.17.21",
		"@types/express-useragent": "1.0.5",
		"@types/ffprobe": "1.1.8",
		"@types/ffprobe-static": "2.0.3",
		"@types/jest": "29.5.12",
		"@types/node": "20.11.17",
		"@types/nodemailer": "6.4.14",
		"@types/opentype.js": "1.3.8",
		"@types/shelljs": "0.8.15",
		"@types/showdown": "2.0.6",
		"@types/uuid": "9.0.8",
		"@typescript-eslint/eslint-plugin": "6.21.0",
		"@typescript-eslint/parser": "6.21.0",
		"eslint": "8.56.0",
		"eslint-config-google": "0.14.0",
		"eslint-plugin-node": "11.1.0",
		"jest": "29.7.0",
		"rimraf": "5.0.5",
		"shelljs": "0.8.5",
		"socket.io-client": "4.7.4",
		"sqlite3": "5.1.7",
		"tailwind-highlightjs": "2.0.1",
		"tailwindcss": "3.3.7",
		"ts-jest": "29.1.2",
		"ts-loader": "9.5.1",
		"ts-node": "10.9.2",
		"typescript": "5.3.3",
		"webpack": "5.89.0",
		"webpack-cli": "5.1.4",
		"workbox-cacheable-response": "7.0.0",
		"workbox-expiration": "7.0.0",
		"workbox-routing": "7.0.0",
		"workbox-strategies": "7.0.0"
	},
	"files": [
		"all"
	],
	"scripts": {
		"dev": "run-p dev:serve dev:js-webpack dev:tailwind-blog dev:tailwind-cms dev:tailwind-cloudcenter dev:assets",
		"debug": "run-p debug:serve dev:js-webpack dev:tailwind-blog dev:tailwind-cms dev:tailwind-cloudcenter dev:assets",
		"test": "jest",
		"coverage": "jest --coverage",
		"build": "run-s clean tsc tailwind-blog tailwind-cms tailwind-cloudcenter copy-assets js-webpack",
		"start": "node .",
		"clean": "rimraf dist/*",
		"copy-assets": "ts-node tools/copyAssets",
		"lint": "eslint src/**/*{.js,.jsx,.ts,.tsx}",
		"lint:fix": "eslint src/**/*{.js,.jsx,.ts,.tsx} --fix",
		"tsc": "tsc",
		"tailwind-blog": "TW_COMPILE_SUBAPP_NAME=blog tailwindcss -i ./src/blog/blog.css -o ./dist/blog/public/blog.css --minify",
		"tailwind-cms": "TW_COMPILE_SUBAPP_NAME=cms tailwindcss -i ./src/cms/cms.css -o ./dist/cms/public/cms.css --minify",
		"tailwind-cloudcenter": "TW_COMPILE_SUBAPP_NAME=cloudcenter tailwindcss -i ./src/cloudcenter/cloudcenter.css -o ./dist/cloudcenter/public/cloudcenter.css --minify",
		"js-webpack": "webpack-cli --devtool source-map",
		"debug:serve": "NODE_ENV=development node --inspect --trace-deprecation --watch --watch-preserve-output -r ts-node/register ./src/wrapper.ts",
		"dev:serve": "NODE_ENV=development node --trace-deprecation --watch --watch-preserve-output -r ts-node/register ./src/wrapper.ts",
		"dev:assets": "node --watch-preserve-output --watch-path ./src/blog/public --watch-path ./src/cms/public --watch-path ./src/cloudcenter/public -r ts-node/register tools/copyAssets.ts",
		"dev:js-webpack": "webpack-cli --devtool inline-source-map --watch --progress",
		"dev:tailwind-blog": "TW_COMPILE_SUBAPP_NAME=blog tailwindcss -i ./src/blog/blog.css -o ./dist/blog/public/blog.css --watch",
		"dev:tailwind-cms": "TW_COMPILE_SUBAPP_NAME=cms tailwindcss -i ./src/cms/cms.css -o ./dist/cms/public/cms.css --watch",
		"dev:tailwind-cloudcenter": "TW_COMPILE_SUBAPP_NAME=cloudcenter tailwindcss -i ./src/cloudcenter/cloudcenter.css -o ./dist/cloudcenter/public/cloudcenter.css --watch"
	},
	"license": "GPLv3"
}
