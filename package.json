{
	"name": "express-blog",
	"version": "1.0.0",
	"description": "",
	"main": "dist/wrapper.js",
	"scripts": {
		"dev": "npm-run-all --parallel dev:serve dev:js-webpack dev:js-move dev:tailwind-blog dev:tailwind-cms dev:tailwind-cloudcenter dev:assets",
		"debug": "npm-run-all --parallel debug:serve dev:js-webpack dev:js-move dev:tailwind-blog dev:tailwind-cms dev:tailwind-cloudcenter dev:assets",
		"test": "jest",
		"coverage": "jest --coverage",
		"build": "npm-run-all clean lint tsc tailwind-blog tailwind-cms tailwind-cloudcenter copy-assets js-wrapper",
		"start": "node .",
		"clean": "rimraf dist/*",
		"copy-assets": "ts-node tools/copyAssets",
		"lint": "eslint src/**/*{.js,.jsx,.ts,.tsx}",
		"tsc": "tsc",
		"tailwind-blog": "TW_COMPILE_SUBAPP_NAME=blog tailwindcss -i ./src/blog/blog.css -o ./dist/blog/public/blog.css --minify",
		"tailwind-cms": "TW_COMPILE_SUBAPP_NAME=cms tailwindcss -i ./src/cms/cms.css -o ./dist/cms/public/cms.css --minify",
		"tailwind-cloudcenter": "TW_COMPILE_SUBAPP_NAME=cloudcenter tailwindcss -i ./src/cloudcenter/cloudcenter.css -o ./dist/cms/public/cloudcenter.css --minify",
		"js-wrapper": "npm-run-all js-webpack js-move",
		"js-webpack": "webpack-cli --devtool source-map",
		"js-move": "ts-node tools/jsBrowserAfterCompile",
		"debug:serve": "nodemon --inspect --watch src --ignore src/browser -e ts ./src/wrapper.ts",
		"dev:serve": "nodemon --watch src --ignore src/browser -e ts ./src/wrapper.ts",
		"dev:assets": "nodemon --watch 'src/blog/public/**/*' --watch 'src/cms/public/**/*' --watch 'src/cloudcenter/public/**/*' --exec ts-node tools/copyAssets",
		"dev:js-webpack": "webpack-cli --devtool inline-source-map --watch --progress",
		"dev:js-move": "nodemon --watch 'dist/public/**/*' --exec ts-node tools/jsBrowserAfterCompile",
		"dev:tailwind-blog": "TW_COMPILE_SUBAPP_NAME=blog tailwindcss -i ./src/blog/blog.css -o ./dist/blog/public/blog.css --watch",
		"dev:tailwind-cms": "TW_COMPILE_SUBAPP_NAME=cms tailwindcss -i ./src/cms/cms.css -o ./dist/cms/public/cms.css --watch",
		"dev:tailwind-cloudcenter": "TW_COMPILE_SUBAPP_NAME=cloudcenter tailwindcss -i ./src/cloudcenter/cloudcenter.css -o ./dist/cloudcenter/public/cloudcenter.css --watch"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"bcrypt": "^5.0.1",
		"body-parser": "^1.20.0",
		"cheerio": "^1.0.0-rc.10",
		"cookie-parser": "^1.4.6",
		"dotenv": "^16.0.0",
		"ejs": "^3.1.6",
		"express": "^4.17.3",
		"express-useragent": "^1.0.15",
		"helmet": "^5.0.2",
		"heroicons": "^1.0.6",
		"moment": "^2.29.3",
		"mysql2": "^2.3.3",
		"npm-run-all": "^4.1.5",
		"pureimage": "^0.3.8",
		"reflect-metadata": "^0.1.13",
		"sequelize": "^6.17.0",
		"sequelize-typescript": "^2.1.3",
		"showdown": "^2.1.0",
		"uuid": "^8.3.2",
		"winston": "^3.6.0"
	},
	"devDependencies": {
		"@tailwindcss/forms": "0.5.2",
		"@tailwindcss/typography": "0.5.2",
		"@types/bcrypt": "5.0.0",
		"@types/cookie-parser": "1.4.3",
		"@types/express": "4.17.13",
		"@types/express-useragent": "1.0.2",
		"@types/jest": "27.5.0",
		"@types/node": "16.11.39",
		"@types/opentype.js": "1.3.3",
		"@types/shelljs": "0.8.11",
		"@types/showdown": "2.0.0",
		"@types/tailwindcss": "3.0.9",
		"@types/uuid": "8.3.4",
		"@typescript-eslint/eslint-plugin": "5.21.0",
		"@typescript-eslint/parser": "5.21.0",
		"eslint": "8.14.0",
		"eslint-config-google": "0.14.0",
		"highlight.js": "11.5.1",
		"jest": "28.0.3",
		"nodemon": "2.0.15",
		"rimraf": "3.0.2",
		"shelljs": "0.8.5",
		"sqlite3": "4.2.0",
		"tailwind-highlightjs": "2.0.0",
		"tailwindcss": "3.1.0",
		"ts-jest": "28.0.1",
		"ts-loader": "9.3.0",
		"ts-node": "10.7.0",
		"typescript": "4.6.2",
		"webpack": "5.70.0",
		"webpack-cli": "4.9.2",
		"workbox-cacheable-response": "6.5.3",
		"workbox-expiration": "6.5.3",
		"workbox-routing": "6.5.3",
		"workbox-strategies": "6.5.3"
	}
}
